{
  "name": "q-neop/blog",
  "description": "a package that provides main features for blog system",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Neop\\Blog\\": "src/",
      "Neop\\Blog\\Factories\\": "database/factories",
      "Neop\\Blog\\Seeders\\": "database/seeders"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neop\\Blog\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "neop team"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/contracts": "^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.3"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Neop\\Blog\\Providers\\BlogServiceProvider"
      ]
    }
  }
}
